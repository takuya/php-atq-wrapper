            "Helpers/helpers.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "takuya/process": "^1.3"
    }
}
