{
    "name": "takuya/php-atq-wrapper",
    "description": "atq/atrm php wrapper class",
    "type": "library",
    "homepage": "https://github.com/takuya/php-atq-wrapper",
    "license": "GPL-3.0-or-later",
    "version": "1.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "takuya",
            "email": "takuya.1stnospam+nospam@gmail.com"
        }
    ],
    "keywords": [
        "atq",
        "atrm",
        "atd",
        "schedule"
    ],

    "autoload": {
        "psr-4": {
            "SystemUtil\\": "src"
        }
    },
